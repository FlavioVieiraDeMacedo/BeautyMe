<link href="~/Content/cssDoGiovani.css" rel="stylesheet" />
@model IEnumerable<BeautyMe.Models.Servico>

@{
    ViewBag.Title = "Index";
}
<div style="margin-top:50px;">
    <header class="partner-header">
        <div class="partner-header--box">
            <div class="partner-header--box__iconbox">
                
            </div>
            <div class="partner-header--box__info">
                @if (Model.Count() > 0)
                {
                    <p class="partner-header--box__infotitle">@Model.First().Profissional.Name</p>
                    <p>@Model.First().Profissional.Email</p>
                    <p>@Model.First().Profissional.Cidade</p>
                }
                else
                {
                    <p class="partner-header--box__infotitle">Olá</p>
                    <p>Voçe ainda não possui serviços cadastrados</p>   
                }
            </div>
            <div class="partner-header--box__logobox">
                <img class="partner-header--box__logo" src="data:image/jpeg;base64," alt="">
            </div>
        </div>
    </header>
    <main>
        <div class="home-content">
            <a href="@Url.Action("Create")">
                <i class="glyphicon glyphicon-plus"></i>
                Adicionar Serviço
            </a>
        </div>
        <div class="hr"></div>

        <div class="home-content">
            <div class="service-list">
                @foreach (var item in Model)
                {
                    <div class="service">
                        <div class="service--info">
                            <p class="service--info__title">@Html.DisplayFor(modelItem => item.Name)</p>
                            <p class="service--info__price">R$ @Html.DisplayFor(modelItem => item.Preco)</p>
                            <p class="service--info__data">Tratamento de: @Html.DisplayFor(modelItem => item.Tempo)</p>
                            <div class="service--actions">
                                @Html.ActionLink("Editar", "Edit", new { id = item.Id }, new { @class = "service--actions__link" })
                                @Html.ActionLink("Remover", "Delete", new { id = item.Id }, new { @class = "service--actions__link" })
                            </div>
                        </div>
                        <div class="service--imagebox">
                            <img class="service--image" src="data:image/jpeg;base64," alt="">
                        </div>
                    </div>
                }
            </div>
        </div>
    </main>
    </div>
    <style>
        .body-content {
            padding-left: 0px;
            padding-right: 0px;
        }
    </style>

